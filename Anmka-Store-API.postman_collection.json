			"name": "Authentication",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.token) {",
									"        pm.environment.set('token', jsonData.token);",
									"        pm.environment.set('user_id', jsonData.user.id);",
									"        pm.environment.set('user_name', jsonData.user.name);",
									"        console.log('Token saved successfully');",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"admin\",\n    \"password\": \"password\",\n    \"website\": \"https://demo1.anmka.com\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/wp-json/anmka/v1/auth/login",
							"host": ["{{base_url}}"],
							"path": ["wp-json", "anmka", "v1", "auth", "login"]
						}
