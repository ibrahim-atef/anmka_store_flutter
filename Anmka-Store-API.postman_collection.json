	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Check if token exists for protected routes",
					"const protectedRoutes = ['/products', '/orders', '/customers', '/dashboard', '/coupons', '/shipping', '/notifications', '/settings'];",
					"const currentUrl = pm.request.url.toString();",
					"const isProtected = protectedRoutes.some(route => currentUrl.includes(route));",
					"",
					"if (isProtected && !pm.environment.get('token')) {",
					"    console.warn('Warning: No token found. Please login first.');",
					"}"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Global test script",
					"pm.test('Response time is less than 5000ms', function () {",
					"    pm.expect(pm.response.responseTime).to.be.below(5000);",
					"});",
					"",
					"// Check for common error codes",
					"if (pm.response.code === 401) {",
					"    console.warn('Unauthorized: Token may be expired. Please login again.');",
					"}",
					"",
					"if (pm.response.code === 403) {",
					"    console.warn('Forbidden: Check if you have proper permissions.');",
					"}"
				]
			}
		}
	]
